<%- include('./layout/head') %>
    <%- include('./layout/navbar') %>
        <div style="margin-top: 20px;" class="content-wrapper">

            <div class="content-header">
                <div class="container">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">
                                <%= tittle %>
                            </h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item">
                                    <a href="/admin">Dashboard</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a href="/admin/jadwal_dokter">Jadwal Dokter</a>
                                </li>
                                <li class="breadcrumb-item active">
                                    <%= tittle %>
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>

            <div class="content-wrapper">

                <div class="content-header">
                    <div class="container">
                        <div align="center">

                            <form action="/admin/jadwal_dokter/save_jadwal" method="post">


                                <div class="form-group col-sm-6" align="left">
                                    <label for="DoctorId">Doctor</label>
                                    <select name="DoctorId" id="DoctorId" class="form-control">
                                        <option value="">--PILIH--</option>
                                        <% docter.forEach(el=>{ %>
                                            <option value="<%= el.id %>">
                                                <%= el.name %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>

                                <div class="form-group col-sm-6" align="left">
                                    <label for="DayId">Hari</label>
                                    <select name="DayId" id="DayId" class="form-control">
                                        <option value="">--PILIH--</option>
                                        <% day.forEach(el=>{ %>
                                            <option value="<%= el.id %>">
                                                <%= el.name %>
                                            </option>
                                            <% }) %>
                                    </select>
                                </div>





                                <button type="submit" class="btn btn-primary">Submit</button>
                            </form>
                        </div>
                    </div>


                </div>
            </div>
        </div>
        <%- include('./layout/footer') %>